{
    "manifest_version": 3,
    "name": "AliExpress Auto Login",
    "version": "1.0",
    "permissions": [
      "scripting",
      "activeTab",
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_title": "AliExpress Login"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://*.aliexpress.com/*", "https://accounts.google.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  