{
    "manifest_version": 3,
    "name": "Open Liepajniekiem.lv & Google Login",
    "version": "1.1",
    "description": "Opens the Liepajniekiem.lv website and clicks the Google login button on Aliexpress.",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "tabs",
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ]
  }
  